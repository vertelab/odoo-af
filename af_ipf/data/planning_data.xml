<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
		<record id="ipf_planning" model="af.ipf">
			<field name="name">IPF Planning</field>
			<field name="environment">T2</field>
			<field name="systemid">AFCRM</field>
			<field name="url">https://ipfapi.arbetsformedlingen.se</field>
			<field name="port">443</field>
		</record>
		<record id="ipf_endpoint_planning" model="af.ipf.endpoint">
			<field name="name">arbetssokande-planering/v1/no-jwt/handlingsplaner?personnummer={personnummer}&amp;client_id={clientid}&amp;client_secret={client_secret}</field>
			<field name="ipf_id" ref="ipf_planning"/>
	</record>
	</data>
</odoo>