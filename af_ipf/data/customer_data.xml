<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <record id="ipf_customer" model="af.ipf">
            <field name="name">IPF Customer</field>
            <field name="environment">T2</field>
            <field name="systemid">AFCRM</field>
            <field name="url">https://ipfapi.arbetsformedlingen.se</field>
            <field name="port">443</field>
            <field name="ssl_verify">True</field>
        </record>
    </data>
    <data>
        <record id="ipf_endpoint_customer" model="af.ipf.endpoint">
            <field name="name">customer/v1/customers/ids?customer_id={customer_id}&amp;client_id={clientid}&amp;client_secret={client_secret}</field>
            <field name="ipf_id" ref="ipf_customer"/>
        </record>
    </data>
</odoo>
