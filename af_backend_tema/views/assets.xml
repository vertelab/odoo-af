<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="_assets_utils" name="Mixins" inherit_id="web._assets_utils" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/mixins.scss"/>
        </xpath>
    </template>

    <template id="_assets_primary_variables" name="Colors" inherit_id="web._assets_primary_variables" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/colors.scss"/>
        </xpath>
    </template>

    <template id="_assets_backend_helpers" name="Variables" inherit_id="web._assets_backend_helpers" priority="15">
        <xpath expr="//link[@href='/web/static/src/scss/bootstrap_overridden.scss']" position="after">
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/variables.scss"/>
        </xpath>
    </template>

    <template id="assets_common" name="Common Assets" inherit_id="web_editor.assets_common">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/fonts.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/af_backend_tema/static/src/js/core/search.js"/>
        </xpath>
    </template>

    <template id="assets_common" name="AF Common Tema" inherit_id="web.assets_common">
        <xpath expr=".">
            <link rel="stylesheet" href="/af_backend_tema/static/src/scss/web_style.scss"/>
        </xpath>
    </template>

    <template id="assets_backend" name="Backend Assets" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
     		<link rel="stylesheet" type="text/css" href="/af_backend_tema/static/src/libs/simplebar/simplebar.css"/>
<!--     		<script type="text/javascript" src="/af_backend_tema/static/src/js/lib/scrollbar.js" />-->
            <link rel="stylesheet" href="/af_backend_tema/static/src/scss/style.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/layout.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/navbar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/apps.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/appsbar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/control_panel.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/search_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/list_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/kanban_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/form_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/fields.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/notification.scss"/>
            <link rel="stylesheet" type="text/scss" href="/af_backend_tema/static/src/scss/settings.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/af_backend_tema/static/src/libs/simplebar/scrollbar.js" />
            <script type="text/javascript" src="/af_backend_tema/static/src/js/chrome/actions.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/chrome/menu.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/chrome/apps.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/chrome/appsbar.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/chrome/sidebar.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/chrome/pager.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/fields/relational.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/views/search_view.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/views/form_view.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/views/form_renderer.js"/>
            <script type="text/javascript" src="/af_backend_tema/static/src/js/views/list_renderer.js"/>
        </xpath>
    </template>

</odoo>
