<?xml version="1.0"?>
<odoo>
    <record id="view_errand_tree" model="ir.ui.view">
        <field name="name">arbetssokande.genomforande.tree</field>
        <field name="model">arbetssokande.genomforande</field>
        <field name="arch" type="xml">
            <tree string="Genomforande" default_order="start desc">
                <field name="source"/>
                <field name="refers_to"/>
                <field name="status"/>
                <field name="start"/>
                <field name="stop"/>
                <field name="extent"/>
                <field name="organiser"/>
            </tree>
        </field>
    </record>
    <record id="view_jobseeker_form" model="ir.ui.view">
        <field name="name">arbetssokande.form.jobseeker.genomforande</field>
        <field name="model">arbetssokande</field>
        <field name="inherit_id" ref="arbetssokande.view_arbetssokande_form"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page name="genomforande" string="Genomforande">
                    <field name="genomforande_ids"/>
                </page>
            </notebook>
        </field>
    </record>
</odoo>
