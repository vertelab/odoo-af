<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="leave_work_schedule" model="hr.leave.type">
        <field name="name">Ej schemalagd</field>
        <field name="repeat" eval="True"/>
        <field name="allocation_type">no</field>
        <field name="request_unit">hour</field>
        <field name="time_type">other</field>
    </record>

    <record id="hr_holidays.holiday_status_cl" model="hr.leave.type">
        <field name="active" eval="False"/>
    </record>

    <record id="hr_holidays.holiday_status_sl" model="hr.leave.type">
        <field name="active" eval="False"/>
    </record>

    <record id="hr_holidays.holiday_status_comp" model="hr.leave.type">
        <field name="active" eval="False"/>
    </record>

    <record id="hr_holidays.holiday_status_unpaid" model="hr.leave.type">
        <field name="active" eval="False"/>
    </record>

    <menuitem id="menu_schedule"
              name="Schedule"
              parent="calendar_af.menu_calendar"
              action="hr_holidays.hr_leave_action_my"
              sequence="25"
              groups="af_security.af_meeting_planner,af_security.af_meeting_admin,af_security.af_jobseekers_officer"/>

</odoo>


